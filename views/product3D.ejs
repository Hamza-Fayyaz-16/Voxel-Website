								<!-- <img src="./img/product01.png" alt=""> -->
							</div>

								<script>		

								// Load 3D Scene
								var scene = new THREE.Scene(); 

								// Load Camera Perspektive
								var camera = new THREE.PerspectiveCamera( 25, window.innerWidth / window.innerHeight, 1, 20000 );
								//var camera = new THREE.PerspectiveCamera( 25, $('#scriptloader').width() / $('#scriptloader').height(), 1, 20000 );
								camera.position.set( 1, 1, 10 );

								// Load a Renderer
								var renderer = new THREE.WebGLRenderer({ alpha: false });
								renderer.setClearColor( 0xC5C5C3 );
								renderer.setPixelRatio( window.devicePixelRatio );
								renderer.setSize($('#scriptloader').width(),450);
								$('#scriptloader').append(renderer.domElement);

								// Load the Orbitcontroller
								var controls = new THREE.OrbitControls( camera, renderer.domElement ); 

								// Load Light
								var ambientLight = new THREE.AmbientLight( 0xcccccc );
								scene.add( ambientLight );

								//front light
								var directionalLight = new THREE.DirectionalLight( 0xffffff );
								directionalLight.position.set( 0, 1, 1 ).normalize();
								directionalLight.intensity = 1;
								scene.add( directionalLight );
								//back light
								var directionalLight2 = new THREE.DirectionalLight( 0xffffff );
								directionalLight2.position.set( 0, 1, -1 ).normalize();
								directionalLight2.intensity = 0.75;
								scene.add( directionalLight2 );				

								// glTf 2.0 Loader
								var object;
								var loader = new THREE.GLTFLoader();				
								loader.load( 'model/ASTRONAUT_HIGH_POLY.gltf', 
								function ( gltf ) {              //<<--------- Model Path 
								object = gltf.scene;				
								gltf.scene.scale.set( 2, 2, 2 );			   
								gltf.scene.position.x = 0;				    //Position (x = right+ left-) 
								gltf.scene.position.y = -1.75;				    //Position (y = up+, down-)
								gltf.scene.position.z = 0;				    //Position (z = front +, back-)

								scene.add( gltf.scene );
								},
								function(xhr){
								console.log((xhr.loaded/xhr.total*100)+'% loaded');
								},
								function(error){
								console.log('An error happened');
								}
								);	 

								function animate() {
								render();
								requestAnimationFrame( animate );
								}

								function render() {
								renderer.render( scene, camera );
								}

								render();
								animate();

								</script>

